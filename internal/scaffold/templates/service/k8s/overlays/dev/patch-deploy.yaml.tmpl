apiVersion: apps/v1
kind: Deployment
metadata:
  name: {{ .Name }}
spec:
  replicas: 1
  template:
    spec:
      containers:
        - name: {{ .Name }}
          env:
            - name: APP_ENV
              value: "development"
          resources:
            requests:
              cpu: "20m"
              memory: "32Mi"
            limits:
              cpu: "200m"
              memory: "128Mi"
